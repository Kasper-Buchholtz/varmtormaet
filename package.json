{
  "name": "second-turbo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "typegen": "cd ./apps/studio && SKIP_VALIDATION=true  sanity schema extract --path ../web/schema.json --enforce-required-fields --workspace home && cd ../web && sanity typegen generate",
    "deploy": "cd ./apps/studio && sanity deploy"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": true,
    "singleQuote": true
  },
  "eslintConfig": {
    "rules": {
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "^(all|part):"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "tailwindcss": "^4.1.12",
    "tailwindcss-debug-screens": "^3.0.1",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "@lottiefiles/react-lottie-player": "^3.5.4",
    "@mynaui/icons-react": "^0.3.2",
    "@next/third-parties": "^15.0.3",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.0",
    "@repo/dashboard": "^0.0.0",
    "@repo/sanity-studio": "^1.4.0",
    "@repo/utils": "*",
    "@sanity/asset-utils": "^2.3.0",
    "@sanity/cli": "^4.6.1",
    "@sanity/client": "^7.11.0",
    "@sanity/dashboard": "^4.1.4",
    "@sanity/demo": "^2.0.0",
    "@sanity/document-internationalization": "^4.0.0",
    "@sanity/icons": "^3.7.4",
    "@sanity/id-utils": "^1.0.0",
    "@sanity/image-url": "^1.2.0",
    "@sanity/locale-da-dk": "^1.0.19",
    "@sanity/orderable-document-list": "^1.4.0",
    "@sanity/presentation": "^2.0.0",
    "@sanity/preview-url-secret": "^2.1.14",
    "@sanity/react-loader": "^1.11.18",
    "@sanity/schema": "^4.6.1",
    "@sanity/types": "^4.6.1",
    "@sanity/ui": "^3.0.8",
    "@sanity/util": "^4.6.1",
    "@sanity/vision": "^4.6.1",
    "@sanity/visual-editing": "^3.0.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/postcss": "^4.1.5",
    "@tailwindcss/typography": "^0.5.15",
    "@tanstack/react-virtual": "^3.13.0",
    "axios": "^1.7.7",
